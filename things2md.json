{
    "area_sep": "//",
    "date_sep": "•",
    "deadline_sep": "⚑",
    "heading_sep": "//",
    "project_sep": "//",
    "skip_tags": "personal,pers,ignore",
    "status_symbols": {
        "canceled": "[x]",
        "completed": "[/]",
        "incomplete": "[ ]"
    },
    "templates": [
        {
            "name": "default",
            "groupby_project": "\n## ☑️ {project}\n",
            "groupby_date": "\n## ☑️ {date}\n",
            "project": "- {status} {title} [↗]({url}) {date} {deadline}",
            "task": "- {status} [[{project}]] {project_sep} {heading} {title} {date_sep} {date} {deadline_sep} {deadline} {tags}",
            "notes": "{notes}",
            "checklist_item": "- {status} {title}"
        },
        {
            "name": "simple",
            "groupby_project": "\n## ☑️ {project}\n",
            "groupby_date": "\n## ☑️ {date}\n",
            "project": "- {status} {title} [↗]({url}) {date} {deadline}",
            "task": "- {status} [[{project}]] {project_sep} {heading} {title} {date_sep} {date} {deadline_sep} {deadline} {tags}",
            "notes": "",
            "checklist_item": ""
        },
        {
            "name": "projects",
            "groupby_project": "\n## ☑️ {project}\n",
            "groupby_date": "\n## ☑️ {date}\n",
            "project": "- {title} [↗]({url}) {date} {deadline} (area::{area}) (taglist::{tags})",
            "task": "- {status} [[{project}]] {project_sep} {heading} {title} {date_sep} {date} {deadline_sep} {deadline} {tags}",
            "notes": "",
            "checklist_item": ""
        },
        {
            "name": "note",
            "type": "markdown_note",
            "title": "## {title}\n\n",
            "body": "{notes}\n\n{checklist}\n\n---\n"
        }
    ]
}